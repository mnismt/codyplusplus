{
  "git": {
    "commitMessage": "chore(release): ${version}",
    "tagName": "v${version}",
    "requireCleanWorkingDir": true
  },
  "github": {
    "release": true,
    "releaseName": "v${version}"
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md",
      "strictLatest": true,
      "addUnreleased": true,
      "keepUnreleased": false,
      "addVersionUrl": false
    }
  }
}
